	public void render(InterfaceElement element, float posX, float posY) {
		if(element.graphics() != null) {
			posX += element.position().getX();
			posY += element.position().getY();
			float[] matrix = getTransformationMatrix(posX,posY,element.graphics().scale().getX(),element.graphics().scale().getY(),element.graphics().rotation().getRotation());
			
			if(element.isWorldElement()) {
				matrix = multiply(getViewMatrix(camera), matrix);
			} else {
				matrix = multiply(getAspectMatrix(), matrix);
			}
			
			program.loadFloat(uniform_opacity, element.graphics().getOpacity());
			program.loadMatrix(uniform_transform, matrix);
			element.graphics().getModel().render();
		}
		
		if(element.hasChildren()) {
			for(int i = 0; i < element.getChildren().size(); i++) {
				render(element.getChildren().get(i), posX, posY);
			}
		}
	}
	
	public void render(InterfaceManager manager) {
		for(Entry<InterfaceElement, DynamicLocation> entry : manager.getElements().entrySet()) {
		    InterfaceElement element = entry.getKey();
		    DynamicLocation location = entry.getValue();
		    render(entry.getKey(), manager.getX(element, location, getFrameWidth()), manager.getY(element, location, getFrameHeight()));
		}
	}